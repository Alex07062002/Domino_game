<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="pane" prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.FXML_files.Start">

   <VBox layoutX="61.0" prefHeight="276.0" prefWidth="250.0" spacing="10.0">
      <Label text="Domino game" textFill="#30d5c8">
         <font>
            <Font name="System Italic" size="24.0"/>
         </font>
      </Label>
      <Button fx:id="StartGame" mnemonicParsing="false" onAction="#StartGamePushed" prefWidth="150.0"
              text="Начало игры">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="Help" mnemonicParsing="false" onAction="#HelpPushed" prefWidth="150.0" text="Справка">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="Quit" mnemonicParsing="false" onAction="#QuitPushed" prefWidth="150.0" text="Выход">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
   </VBox>
</AnchorPane>
